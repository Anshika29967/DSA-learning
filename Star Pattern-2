#include <iostream>
using namespace std;


int main() {
    // Write C++ code here
    int n=5;
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++)
        {
            cout<<"*";
        }
        cout<<endl;
    }
    return 0;
}


# Output of the above code will be -----

*****
****
***
**
*
